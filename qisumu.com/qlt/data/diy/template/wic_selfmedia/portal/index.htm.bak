<script type="text/javascript">
	jq(".index_slide").slide({ mainCell:".bd ul",titCell:".hd li",delayTime:600,interTime:5000,triggerTime:50,effect:"leftLoop",autoPlay:true,});
	jq(".index_slide").hover(function(){
	jq(".index_slide .slide-nav").fadeIn();
	},function(){
	jq(".index_slide .slide-nav").fadeOut();
	})
	jq(".index_news_c").slide({ mainCell:".bd ul", effect:"topLoop", delayTime:600,interTime:4000,vis:7,scroll:7,trigger:"click",
	startFun:function(){
        jq(".change").addClass("push");
    },
    endFun:function(){
        jq(".change").removeClass("push");
    }});
	jq(".index_topic_c").slide({ mainCell:".bd ul",titCell:".hd li",delayTime:400,interTime:5000,triggerTime:50,effect:"leftLoop",autoPlay:true,});
	jq(".index_topic_c").hover(function(){
	jq(".index_topic_c .slide-nav").fadeIn();
	},function(){
	jq(".index_topic_c .slide-nav").fadeOut();
	})
</script>


